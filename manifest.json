{
  "name" : "Undirect",
  "version" : "1.0",
  "description" : "Removes redirection tracking of clicks on search results in google",
  "icons": {
    "16": "undirect48.png",
    "48": "undirect48.png",
    "128": "undirect128.png"
  },
  "content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["undirect.js"],
      "run_at" : "document_idle",
      "all_frames" : false
    }
  ]
}
